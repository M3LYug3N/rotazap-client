{
  "name": "rotazap-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development bunx next dev",
    "dev:turbo": "NODE_ENV=development bunx next dev --turbopack",
    "build": "bun run check:heavy && bun run lint && bunx next build",
    "build:analyze": "bun run check:heavy && bun run lint && ANALYZE=true bunx next build",
    "build:turbo": "bun run check:heavy && bun run lint && bunx next build --turbopack",
    "analyze": "ANALYZE=true bunx next build",
    "start": "NODE_ENV=production bunx next start",
    "lint": "bunx next lint",
    "lint:css": "bunx stylelint \"**/*.css\" --cache --allow-empty-input",
    "lint:css:fix": "bunx stylelint \"**/*.css\" --fix",
    "check:heavy": "bun scripts/find-heavy-components.mjs",
    "check:cycles": "bunx depcruise app --output-type err",
    "check:cycles:graph": "bunx depcruise app --output-type dot | dot -T svg > dep-graph.svg",
    "check:deps": "ncu",
    "upgrade:deps": "ncu -u && bun install"
  },
  "dependencies": {
    "next": "15.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@next/bundle-analyzer": "^15.5.4",
    "@react-input/mask": "^2.0.4",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "cookies-next": "^6.1.0",
    "date-fns": "^4.1.0",
    "dependency-cruiser": "^17.0.2",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "file-saver": "^2.0.5",
    "gsap": "^3.13.0",
    "js-cookie": "^3.0.5",
    "npm-check-updates": "^19.0.0",
    "postcss": "^8.5.6",
    "postcss-functions": "^4.0.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-datepicker": "^8.7.0",
    "react-hook-form": "^7.64.0",
    "react-scan": "^0.4.3",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "stylelint": "^16.25.0",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4.1.14",
    "typescript": "^5",
    "xlsx": "^0.18.5",
    "yup": "^1.7.1",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  }
}
